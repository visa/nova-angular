{
  "name": "nova-angular-project",
  "version": "5.1.3",
  "description": "Visa Product Design System Nova Angular Project",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "api:docs": "compodoc -p apps/workshop/tsconfig.json --hideGenerator -e json -d apps/workshop/src/assets/app/workshop",
    "api:lib": "compodoc -p libs/nova-lib/tsconfig.lib.json --hideGenerator -e json -d apps/workshop/src/assets/lib/nova",
    "api": "pnpm api:lib && pnpm api:docs",
    "docs:build": "nx run workshop:build",
    "docs:build:dev": "nx run workshop:build:development",
    "docs:build:prod": "nx run workshop:build:production",
    "docs:serve": "nx run workshop:serve:development --open --watch",
    "docs:serve:dev": "nx run workshop:serve:development --open --watch",
    "docs:serve:prod": "nx run workshop:serve:production --open --watch",
    "generate-routes": "node bin/route-gen.mjs",
    "lib:build": "nx run nova-lib:build:production",
    "lib:build:dev": "nx run nova-lib:build:development",
    "lib:build:prod": "nx run nova-lib:build:production",
    "lib:pub": "pnpm lib:build && cd dist/libs/nova-lib && npm publish --registry https://artifactory.trusted.visa.com/api/npm/Visa_Commons_npm/",
    "preinstall": "npx only-allow pnpm",
    "prettier:docs:write": "prettier --config ./.prettierrc --check --write \"apps/workshop/src/app/components/*/*/*{.ts,.html}\"",
    "release": "standard-version && pnpm copy:changelog",
    "release:minor": "standard-version --release-as minor && pnpm copy:changelog",
    "release:patch": "standard-version --release-as patch && pnpm copy:changelog",
    "generate:metadata": "node bin/metadata/create-meta-data",
    "generate:themes": "node bin/generate-themes.mjs",
    "start": "nx run workshop:build",
    "test": "nx test nova-lib",
    "test:coverage": "npx jest nova-lib --coverage",
    "test:coverage:watch": "npx jest nova-lib --coverage --watchAll",
    "test:watch": "nx test nova-lib --watchAll",
    "test:serve": "cd coverage/lcov-report && npx http-serve",
    "copy:changelog": "cp CHANGELOG.md apps/workshop/src/assets/lib/nova/CHANGELOG.md",
    "prepare": "pnpm generate-routes && pnpm generate:themes && pnpm api && is-ci || husky",
    "license:update": "npx @visa/scripts license"
  },
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/forms": "^17.3.10",
    "@angular/router": "^17.3.10",
    "@floating-ui/dom": "1.6.2",
    "@visa/nova-icons-angular": "^1.6.5",
    "@visa/nova-styles": "1.5.8",
    "change-case": "^5.4.4",
    "clipboard": "2.0.11",
    "core-js": "3.33.1",
    "dompurify": "3.2.4",
    "marked": "^12",
    "ngx-clipboard": "16.0.0",
    "ngx-google-analytics": "14.0.1",
    "ngx-markdown": "^17",
    "ngx-window-token": "7.0.0",
    "prismjs": "^1.29.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular/compiler": "^17.3.10",
    "@angular/compiler-cli": "^17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@compodoc/compodoc": "^1.1.24",
    "@floating-ui/core": "1.6.2",
    "@floating-ui/utils": "0.2.2",
    "@nx/angular": "^18.0.5",
    "@nx/eslint-plugin": "^18.0.5",
    "@nx/jest": "^18.0.5",
    "@nx/linter": "^18.0.5",
    "@nx/workspace": "^18.0.5",
    "@testing-library/angular": "^17.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.12.13",
    "@typescript-eslint/parser": "^7.11.0",
    "chokidar": "^3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^10.4.1",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-preset-angular": "^14.1.0",
    "ng-packagr": "^17.3.0",
    "nx": "^18.0.5",
    "only-allow": "^1.2.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "terminal-kit": "^3.1.1",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=18.15.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.12.0",
  "keywords": [
    "angular",
    "design system",
    "javascript",
    "nova",
    "nx",
    "typescript",
    "ui",
    "visa",
    "visa design",
    "visa design system"
  ],
  "publishConfig": {
    "@visa:registry": "https://artifactory.trusted.visa.com/api/npm/Visa_Commons_npm/"
  }
}
